<div id="map"></div>
<script type="text/javascript">//<![CDATA[
var maps = ["label", "bus", "bento"], map = maps[0], locale = "ja";
if (document.cookie) {
  var cookies = document.cookie.split("; ");
  for (var i = 0; i < cookies.length; i++) {
    var str = cookies[i].split("=");
    if (str[0] == "map") {
      var cookie_value = unescape(str[1]);
      if (maps.contains(cookie_value)) {
        map = cookie_value;
      }
      break;
    } else if (str[0] == "locale") {
      var cookie_value = unescape(str[1]);
      if (cookie_value == "en") locale = cookie_value;
    }
  }
}
function change_map(map) {
  jQuery('#map').html('<object id="maps" data="svg/map.svg?map=' + map + '" type="image/svg+xml" width="480" height="1000" style="position:absolute;top:60px;margin:0;padding:0;overflow:hidden">You must use an SVG supported web browswer in order for this application to display correctly.<br><br>このアプリを正しく表示するために、SVGに対応したブラウザをご利用ください。</object>');
}
change_map(map);
